{
  "name": "cfrok",
  "version": "0.1.0",
  "description": "Tiny ngrok-like CLI and Node.js library powered by cloudflared",
  "license": "MIT",
  "bin": {
    "cfrok": "dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist/",
    "scripts/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "postinstall": "node scripts/postinstall.js || true"
  },
  "dependencies": {
    "mri": "^1.2.0",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/mri": "^1.1.3",
    "@types/node": "^22.7.5",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "bun@1.1.34"
}
